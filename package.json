{
	"name": "my-webpack-project",
	"version": "1.0.0",
	"description": "React starter with webpack",
	"scripts": {
		"prepare": "husky install && husky add .husky/pre-commit \"npx lint-staged\"",
		"dev": "webpack serve",
		"build:dev": "webpack --mode=development",
		"build:prod": "webpack --mode=production --node-env=production",
		"lint": "eslint --ext .js --ext .jsx src",
		"pretty-quick": "pretty-quick --staged --ignore-path .gitignore",
		"test": "jest",
		"pre-commit": "pretty-quick --staged --ignore-path .gitignore",
		"pre-push": "jest"
	},
	"keywords": [
		"Webpack",
		"React",
		"jest",
		"eslint",
		"prettier",
		"sass",
		"husky"
	],
	"author": "Mohit Patel",
	"license": "ISC",
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.3.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.17.10",
		"@babel/core": "^7.18.5",
		"@babel/eslint-parser": "^7.18.2",
		"@babel/preset-env": "^7.18.2",
		"@babel/preset-react": "^7.17.12",
		"@testing-library/jest-dom": "^5.16.4",
		"@testing-library/react": "^13.3.0",
		"babel-loader": "^8.2.5",
		"css-loader": "^6.7.1",
		"dotenv-webpack": "^7.1.0",
		"eslint": "^8.17.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^8.5.0",
		"eslint-config-react": "^1.1.7",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jest": "^26.5.3",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-react": "^7.30.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-webpack-plugin": "^3.1.1",
		"html-webpack-plugin": "^5.5.0",
		"husky": "^8.0.1",
		"jest": "^28.1.1",
		"jest-environment-jsdom": "^28.1.1",
		"lint-staged": "^13.0.2",
		"mini-css-extract-plugin": "^2.6.1",
		"prettier": "2.7.1",
		"pretty-quick": "^3.1.3",
		"sass": "^1.52.3",
		"sass-loader": "^13.0.0",
		"style-loader": "^3.3.1",
		"url-loader": "^4.1.1",
		"webpack": "^5.73.0",
		"webpack-cli": "^4.10.0",
		"webpack-dev-server": "^4.9.2"
	},
	"lint-staged": {
		"*": "npm run pretty-quick",
		"src/**/*.{js,jsx}": "npm run lint"
	}
}
